      <Query>
        <DataSourceName>PreSIS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        vProvinceByBATeam.RegionName, vProvinceByBATeam.AreaName, vProvinceByBATeam.ProvinceName, SMSDailySalesOutSummary.SaleDate, vItem.BrandName, 
                         vItem.ItemID, vItem.ItemName, vItem.HiddenItem, SMSDailySalesOutSummary.SalesOut_CBG, SMSDailySalesOutSummary.NumberOfBAOnField, 
                         SMSDailySalesOutSummary.NumberOfBAByItem
FROM            SMSDailySalesOutSummary INNER JOIN
                         vProvinceByBATeam ON SMSDailySalesOutSummary.ProvinceID = vProvinceByBATeam.ProvinceID INNER JOIN
                         vItem ON SMSDailySalesOutSummary.ItemID = vItem.ItemID
WHERE        (SMSDailySalesOutSummary.ProvinceID IN (@provinceID)) AND (SMSDailySalesOutSummary.ItemID IN (@itemID)) AND 
                         (SMSDailySalesOutSummary.SaleDate BETWEEN @startDate AND @endDate) AND (vItem.HiddenItem = 2)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@provinceID">
            <Value>=Parameters!provinceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@itemID">
            <Value>=Parameters!itemID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
