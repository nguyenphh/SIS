      <Query>
        <DataSourceName>PreSIS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        DisMonthlyStock.DistributorID, IndexByDistributor.DragonDistributorName, IndexByDistributor.Region, IndexByDistributor.Province, DisMonthlyStock.Year, 
                         DisMonthlyStock.Month, DisMonthlyStockDescription.Description, DisMonthlyStock.ActualQuantity, vItem.BrandName, DisMonthlyStock.ItemID, vItem.ItemName, 
                         DisMonthlyStockDescription.SortOrder
FROM            DisMonthlyStock INNER JOIN
                         IndexByDistributor ON DisMonthlyStock.DistributorID = IndexByDistributor.DistributorID AND DisMonthlyStock.Year = IndexByDistributor.Year AND 
                         DisMonthlyStock.Month = IndexByDistributor.Month INNER JOIN
                         DisMonthlyStockDescription ON DisMonthlyStock.ID = DisMonthlyStockDescription.ID INNER JOIN
                         vItem ON DisMonthlyStock.ProductGroupCode = vItem.ProductGroupCode</CommandText>
        <Timeout>0</Timeout>
      </Query>
