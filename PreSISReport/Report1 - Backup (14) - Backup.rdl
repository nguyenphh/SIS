      <Query>
        <DataSourceName>PreSIS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        vProvinceDistributor.RegionName, vProvinceDistributor.AreaName, vProvinceDistributor.ProvinceName, vDisBuyPortfolio.Year, vDisBuyPortfolio.Month, 
                         vDisBuyPortfolio.ItemID, vDisBuyPortfolio.NumberOfDistributor, vItem.BrandName, vItem.ItemName
FROM            vDisBuyPortfolio INNER JOIN
                         vProvinceDistributor ON vDisBuyPortfolio.ProvinceID = vProvinceDistributor.ProvinceID INNER JOIN
                         vItem ON vDisBuyPortfolio.ItemID = vItem.ItemID
WHERE        (vDisBuyPortfolio.Year = @year) AND (vDisBuyPortfolio.Month = @month)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@month">
            <Value>=Parameters!month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
