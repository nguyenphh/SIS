      <Query>
        <DataSourceName>PreSIS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        IndexByDistributor.Region, IndexByDistributor.Area, IndexByDistributor.Province, IndexByDistributor.ProvinceID, vItem.BrandName, vItem.ItemName, 
                         vDisMonthlyRevenueSalesByInvoice.DistributorID, vDisMonthlyRevenueSalesByInvoice.ItemID, vDisMonthlyRevenueSalesByInvoice.ActualQuantity, 
                         IndexByDistributor.DragonDistributorName, vDisMonthlyRevenueSalesByInvoice.Year, vDisMonthlyRevenueSalesByInvoice.Month
FROM            IndexByDistributor INNER JOIN
                         vDisMonthlyRevenueSalesByInvoice ON IndexByDistributor.Year = vDisMonthlyRevenueSalesByInvoice.Year AND 
                         IndexByDistributor.Month = vDisMonthlyRevenueSalesByInvoice.Month AND 
                         IndexByDistributor.DistributorID = vDisMonthlyRevenueSalesByInvoice.DistributorID INNER JOIN
                         vItem ON vDisMonthlyRevenueSalesByInvoice.ItemID = vItem.ItemID</CommandText>
        <Timeout>0</Timeout>
      </Query>
